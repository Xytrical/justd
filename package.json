{
  "name": "intentui",
  "author": "irsyadadl",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "first-time": "bun i && bun run build && next dev",
    "lint": "biome check",
    "build:stubs": "rm -rf public/stubs && bun run resources/scripts/build-stubs.ts",
    "prebuild": "bun run build:old-registry && bun run build:registry && bun run build:stubs",
    "cuc": "bun run resources/scripts/check-use-client.ts",
    "cdc": "bun run resources/scripts/check-duplicated-classes.ts",
    "prepare": "husky",
    "build:old-registry": "rm -rf public/registry && bun run resources/scripts/build-old-registry.ts",
    "build:registry": "bun run resources/scripts/build-registry.ts && npx shadcn build",
    "dev": "next dev --turbopack",
    "build": "next build",
    "format": "biome lint --fix && biome check --write",
    "format:unsafe": "biome check --unsafe --write",
    "build:everything": "rm -rf pubilc/registry && bun run build:registry",
    "bb": "bun run format && bun run build:everything",
    "start": "next start",
    "preview": "bun run build && next start",
    "ic": "bun run resources/scripts/identify-color.ts",
    "typecheck": "tsc --noEmit",
    "postinstall": "husky"
  },
  "lint-staged": {
    "./**/*.{ts,tsx}": [
      "biome lint --fix && biome check --write"
    ]
  },
  "dependencies": {
    "@intentui/icons": "^1.10.34",
    "@radix-ui/react-scroll-area": "^1.2.6",
    "@shikijs/rehype": "^3.3.0",
    "@shikijs/transformers": "^3.3.0",
    "clsx": "^2.1.1",
    "culori": "^4.0.1",
    "dayjs": "^1.11.13",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "fumadocs-mdx": "11.6.1",
    "input-otp": "^1.4.2",
    "motion": "^12.9.4",
    "next": "^15.3.1",
    "next-themes": "^0.4.6",
    "ntcjs": "^1.1.3",
    "react": "^19.1.0",
    "react-aria-components": "^1.8.0",
    "react-dom": "^19.1.0",
    "react-intersection-observer": "^9.16.0",
    "react-resizable-panels": "^2.1.9",
    "recharts": "^2.15.3",
    "rehype-stringify": "^10.0.1",
    "scroll-into-view-if-needed": "^3.1.0",
    "shadcn": "^2.5.0",
    "sharp": "^0.34.1",
    "shiki": "^3.3.0",
    "sonner": "^2.0.3",
    "tailwindcss-react-aria-components": "^2.0.0",
    "tw-animate-css": "^1.2.9",
    "use-debounce": "^10.0.4",
    "usemods": "^1.13.0"
  },

  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@commitlint/cz-commitlint": "^19.8.0",
    "cz-conventional-changelog": "^3.3.0",
    "@biomejs/biome": "^1.9.4",
    "@tailwindcss/postcss": "^4.1.5",
    "@tailwindcss/typography": "^0.5.16",
    "@types/culori": "^2.1.1",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@types/title": "^4.0.0",
    "babel-plugin-react-compiler": "^19.0.0-beta-ebf51a3-20250411",
    "encoding": "^0.1.13",
    "husky": "^9.1.7",
    "postcss": "^8.5.3",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "tailwind-merge": "^3.2.0",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.5",
    "typescript": "^5.8.3"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/irsyadadl/intentui.git"
  },
  "trustedDependencies": ["es5-ext", "esbuild", "sharp"],
  "overrides": {
    "react-is": "^19.0.0"
  }
}
